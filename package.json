{
  "name": "gdj-fuzion-admin-api",
  "version": "1.0.0",
  "description": "testing fuzion-admin-api",
  "main": "index.js",
  "scripts": {
    "build": "npm run-script mysql-start && sleep 10 && npm run-script mysql-init && npm run-script start",
    "start": "node ./node_modules/fuzion-admin-api/bin/www",
    "mysql-start": "docker run -it --rm -v lmysql:/var/lib/mysql -p 3306:3306 --name fuzion-mysql -e MYSQL_ROOT_PASSWORD=coffeetime -d mysql:5.7.20",
    "mysql-init": "node init_db.js --host localhost --user root --password coffeetime --file ./test_ordered_uuid.sql",
    "mysql-end": "docker stop fuzion-mysql && docker volume rm lmysql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdjenkins19/gdj-fuzion-admin-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gdjenkins19/gdj-fuzion-admin-api/issues"
  },
  "homepage": "https://github.com/gdjenkins19/gdj-fuzion-admin-api#readme",
  "dependencies": {
    "fuzion-admin-api": "git+https://github.com/Klowd/fuzion-admin-api.git#dev",
    "optimist": "^0.6.1"
  }
}
